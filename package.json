{
  "name": "codeflow",
  "version": "1.3.0",
  "description": "Document and Debug Platform.",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/valcas/codeflow.git"
  },
  "homepage": "",
  "bugs": {
    "url": "https://github.com/valcas/codeflow/issues"
  },
  "author": {
    "name": "Val Cassidy",
    "email": "val@codeumentus.com",
    "url": "https://github.com/valcas"
  },
  "presets": [
    "@babel/preset-es2017"
  ],
  "plugins": [
    "transform-object-rest-spread"
  ],
  "keywords": [
    "app",
    "boilerplate",
    "electron",
    "open",
    "open-source",
    "postcss",
    "react",
    "reactjs",
    "source",
    "webpack"
  ],
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "main": "main.js",
  "scripts": {
    "prod": "webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds --asar=true",
    "setup": "electron-installer-windows --src builds/codeflow-win32-x64/ --dest dist/installers/"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "copy-webpack-plugin": "^4.5.2",
    "electron": "^2.0.3",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "json-config": "github:valcas/json-config",
    "material-ui": "^0.20.2",
    "material-ui-tree": "^1.1.1",
    "mxgraph-js": "^1.0.1",
    "node-sass": "^4.12.0",
    "querystring": "^0.2.0",
    "raw-loader": "^0.5.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.0.7",
    "react-scripts": "^1.1.5",
    "recompose": "^0.29.0",
    "redoc": "^2.0.0-rc.4",
    "redux": "^4.0.0",
    "redux-connect": "^8.0.0",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.1.1",
    "swagger-jsdoc": "^3.2.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.11",
    "electron-installer-windows": "^1.1.0",
    "electron-packager": "^12.1.0",
    "electron-winstaller": "^2.6.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "postcss-pxtorem": "^4.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.13.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
